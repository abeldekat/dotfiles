" Plug 'sickill/vim-monokai'
" A fork:
Plug 'justinmk/molokai'

" set background=dark
" if (colorlib#currentLinuxThemeIsLight())
" 	set background=light
" endif

" let g:molokai_original = 1
" let g:rehash256 = 1

let g:my_colorscheme_plugin = "molokai"
let g:lightline = { 'colorscheme': 'molokai' } 

